	Socket _MySocket;
	public Socket _Socket;
	public SocketAddress _SocketAddress;
	public InputStream _InputStream;
	public OutputStream _Outputstream;
	
	
	public void run(UUID MiUI, Socket MySocket) {
		// TODO Auto-generated method stub
		super.run();
		_UI = MiUI;
		_MySocket = MySocket;
		
	}

}
